@model Britespokes.Web.Models.College.StudentInquiry
@{
  Layout = null;
}
<h1>Student Inquiry for @Model.Tour.Permalink</h1>
<h1>School Details</h1>
<table>
  <tr>
    <td>Name</td>
    <td>@Model.Form.SchoolDetails.Name</td>
  </tr>
  <tr>
    <td>Street Address</td>
    <td>@Model.Form.SchoolDetails.StreetAddress</td>
  </tr>
  <tr>
    <td>City / State</td>
    <td>@Model.Form.SchoolDetails.City</td>
  </tr>
  <tr>
    <td>Country</td>
    <td>@Model.Form.SchoolDetails.Country</td>
  </tr>
  <tr>
    <td>Zip Code</td>
    <td>@Model.Form.SchoolDetails.ZipCode</td>
  </tr>
  <tr>
    <td>School Contact Name</td>
    <td>@Model.Form.SchoolDetails.ContactName</td>
  </tr>
  <tr>
    <td>School Contact Telephone</td>
    <td>@Model.Form.SchoolDetails.ContactTelephone</td>
  </tr>
  <tr>
    <td>School Contact Email</td>
    <td>@Model.Form.SchoolDetails.ContactEmail</td>
  </tr>
  <tr>
    <td>Number of Students Traveling</td>
    <td>@Model.Form.SchoolDetails.TravelingStudentCount</td>
  </tr>
</table>
<h1>Student Details</h1>
<table>
  <tr>
    <td>Name</td>
    <td>@Model.Form.StudentDetails.Name</td>
  </tr>
  <tr>
    <td>Birthdate</td>
    <td>@Model.Form.StudentDetails.Birthdate</td>
  </tr>
  <tr>
    <td>Grade</td>
    <td>@Model.Form.StudentDetails.Grade</td>
  </tr>
  <tr>
    <td>Gender</td>
    <td>@Model.Form.StudentDetails.Gender</td>
  </tr>
  <tr>
    <td>Parent / Guardian Name</td>
    <td>@Model.Form.StudentDetails.Guardian1Name</td>
  </tr>
  <tr>
    <td>Parent / Guardian Cell Phone Number</td>
    <td>@Model.Form.StudentDetails.Guardian1MobilePhoneNumber</td>
  </tr>
  <tr>
    <td>Parent / Guardian Business Phone Number</td>
    <td>@Model.Form.StudentDetails.Guardian1BusinessPhoneNumber</td>
  </tr>
  <tr>
    <td>Parent / Guardian Home Phone Number</td>
    <td>@Model.Form.StudentDetails.Guardian1HomePhoneNumber</td>
  </tr>
  <tr>
    <td>Parent / Guardian Email</td>
    <td>@Model.Form.StudentDetails.Guardian1Email</td>
  </tr>
  <tr>
    <td>Parent / Guardian Name</td>
    <td>@Model.Form.StudentDetails.Guardian2Name</td>
  </tr>
  <tr>
    <td>Parent / Guardian Cell Phone Number</td>
    <td>@Model.Form.StudentDetails.Guardian2MobilePhoneNumber</td>
  </tr>
  <tr>
    <td>Parent / Guardian Business Phone Number</td>
    <td>@Model.Form.StudentDetails.Guardian2BusinessPhoneNumber</td>
  </tr>
  <tr>
    <td>Parent / Guardian Home Phone Number</td>
    <td>@Model.Form.StudentDetails.Guardian2HomePhoneNumber</td>
  </tr>
  <tr>
    <td>Parent / Guardian Email</td>
    <td>@Model.Form.StudentDetails.Guardian2Email</td>
  </tr>
  <tr>
    <td>Additional Notes</td>
    <td>@Model.Form.StudentDetails.Notes</td>
  </tr>
</table>